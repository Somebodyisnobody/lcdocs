          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd" xml:lang="de">
	<func isAsync="false">
		<title>PrivateCall</title>
		<category>Script/Funktionsaufruf</category>
		<versions><version>4.6.5.0 CP</version></versions>
		<deprecated>
			<author>DerTod</author>
			<date>2022-09-14</date>
			<version>4.9.10.14 [355] LC</version>
			<description>Statt dieser Funktion sollte die neue Syntax zum Aufrufen von Funktionen verwendet werden. Siehe <emLink href="script/FuncCall.html">Aufruf von Scriptfunktionen</emLink>.</description>
		</deprecated>
		<syntax>
			<rtype isReference="false">any</rtype>
			<params>
				<param isReference="false" isOptional="false"><type>object</type><name>pObj</name><desc>Objekt, in dem die Funktion aufgerufen werden soll.</desc></param>
				<param isReference="false" isOptional="false"><type>string</type><name>szFunction</name><desc>Funktion, die aufgerufen werden soll.</desc></param>
				<param isReference="false" isOptional="false"><type>any</type><name>...</name><desc>Parameter der Funktion.</desc></param>
			</params>
		</syntax>
		<desc>Ruft die private, geschützte oder öffentliche Funktion szFunction in pObj auf. Es sollte überlegt werden, ob der Aufruf wirklich nötig ist.</desc>
		<related>
			<funcLink>ObjectCall</funcLink>
			<funcLink>ProtectedCall</funcLink>
			<funcLink>Call</funcLink>
			<funcLink>GameCall</funcLink>
			<funcLink>eval</funcLink>
		</related>
	</func>
