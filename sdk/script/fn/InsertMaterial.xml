<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<clonkDoc xmlns="https://clonkspot.org"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd" xml:lang="de">
  <func>
    <title>InsertMaterial</title>
    <category>Landschaft/Material</category>
    <versions><version>4.6.5.0 CP</version><extversion>4.9.1.0 GWE</extversion></versions>
    <syntax>
      <rtype>bool</rtype>
      <params>
        <param><type>int</type><name>iMat</name><desc>Einzufügendes Material (siehe <funcLink>Material</funcLink>())</desc></param>
        <param><type>int</type><name>iX</name><desc>X-Einfügeposition/-offset</desc><optional /></param>
        <param><type>int</type><name>iY</name><desc>Y-Einfügeposition/-offset</desc><optional /></param>
        <param><type>int</type><name>iXDir</name><desc>X-Geschwindigkeit des eingefügten Mateiralpixels</desc><optional /></param>
        <param><type>int</type><name>iYDir</name><desc>Y-Geschwindigkeit des eingefügten Mateiralpixels</desc><optional /></param>
      </params>
    </syntax>
    <desc>Fügt ein Materialpixel an der angegebenen Position in der angegebenen Geschwindigkeit ein.</desc>
    <related>
      <funcLink>Material</funcLink>
      <funcLink>ExtractMaterialAmount</funcLink>
    </related>
  </func>
  <author>Sven2</author><date>2001-11</date>
