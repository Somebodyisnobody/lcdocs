  <func isAsync="false">
    <title>LaunchEarthquake</title>
    <category>Umwelt</category>
    <versions><version>4.6.5.0 CP</version></versions>
    <syntax>
      <rtype isReference="false">void</rtype>
      <params>
        <param isReference="false" isOptional="false">
          <type>int</type>
          <name>iX</name>
          <desc>X-Zielposition</desc>
        </param>
        <param isReference="false" isOptional="false">
          <type>int</type>
          <name>iY</name>
          <desc>Y-Zielposition</desc>
        </param>
      </params>
