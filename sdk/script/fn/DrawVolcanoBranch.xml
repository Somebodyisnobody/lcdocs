      <rtype>void</rtype>
      <params>
        <param><type>int</type><name>iMat</name><desc>Index des Materials, das gezeichnet werden soll.</desc></param>
        <param><type>int</type><name>fX</name><desc></desc></param>
        <param><type>int</type><name>fY</name><desc></desc></param>
        <param><type>int</type><name>tX</name><desc></desc></param>
        <param><type>int</type><name>tY</name><desc></desc></param>
