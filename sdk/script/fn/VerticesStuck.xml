<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<clonkDoc xmlns="https://clonkspot.org"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd">
  <func>
    <title>VerticesStuck</title>
    <category>Objekte</category><subcat>Vertices</subcat>
    <versions><version>4.9.1.0 GWE</version></versions>

    <syntax>
      <rtype>int</rtype>
        <params>
          <param>
            <type>object</type>
            <name>pObj</name>
            <desc>Zielobjekt</desc><optional />
          </param>
        </params>
      </syntax>

      <desc>Liefert die Anzahl feststeckender Vertices von *pObj</desc>

      <examples>
        <example>
          <code>var obj=FindObject(FLAG);
if (VerticesStuck(obj)) RemoveObject(obj);</code>
          <text>Entfernt obj wenn einer seiner Vertices feststeckt</text>
        </example>
      </examples>

      <related>
        <funclink>GetVertex</funclink>
        <funclink>Stuck</funclink>
      </related>
  </func>
  <author>flgr</author><date>2002-01</date>
</clonkDoc>

