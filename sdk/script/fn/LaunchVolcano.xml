<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<clonkDoc xmlns="https://clonkspot.org"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd">
  <func>
    <title>LaunchVolcano</title>
    <category>Umwelt</category>
    <versions><version>4.6.5.0 CP</version></versions>
    <syntax>
      <rtype>bool</rtype>
      <params>
        <param>
          <type>int</type>
          <name>iX</name>
          <desc>X-Zielposition</desc>
        </param>
      </params>
    </syntax>
    <desc>Erzeugt einen Vulkan an angegebener Position. Da Vulkane immer von der Unterseite der Landschaft ausgehen, wird hier kein Y-Parameter benötigt.</desc>
    <related>
      <funclink>LaunchLightning</funclink> <funclink>LaunchEarthquake</funclink>
    </related>
  </func>
  <author>PeterW</author><date>2001-11</date>
</clonkDoc>
