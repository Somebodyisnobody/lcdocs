<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<clonkDoc xmlns="https://clonkspot.org"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd" xml:lang="de">
  <func isAsync="false">
    <title>Inside</title>
    <category>Arithmetik</category>
    <versions><version>4.6.5.0 CP</version></versions>
    <syntax>
      <rtype isReference="false">bool</rtype>
      <params>
        <param isReference="false" isOptional="false">
          <type>int</type>
          <name>iVal</name>
          <desc>Eingabewert</desc>
        </param>
        <param isReference="false" isOptional="false">
          <type>int</type>
          <name>iRange1</name>
          <desc>Untergrenze für den Wert</desc>
        </param>
        <param isReference="false" isOptional="false">
          <type>int</type>
          <name>iRange2</name>
          <desc>Obergrenze für den Wert</desc>
        </param>
      </params>
    </syntax>
    <desc>Gibt TRUE zurück, wenn iVal &gt;= iRange1 und iVal &lt;= iRange2 ist.</desc>
    <related>
      <funcLink>BoundBy</funcLink>
    </related>
  </func>
  <author>PeterW</author><date>2001-11</date>
</clonkDoc>
