<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<clonkDoc xmlns="https://clonkspot.org"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd" xml:lang="de">
        <func>
                <title>GlobalN</title>
                <category>Variablen</category>
                <versions><version>4.9.1.0 GWE</version></versions>
                <syntax>
                        <rtype>string</rtype>
                        <isReference />
                        <params>
                                <param><type>string</type><name>szText</name><desc>Name einer globalen Variable.</desc></param>
                        </params>
                </syntax>
                <desc>Liefert den Inhalt der globalen Variable mit dem Namen szText. Per GlobalN("Name")=Wert; kann der Wert einer benannten globalen Variable gesetzt werden.</desc>
                <related>
                        <funcLink>Global</funcLink>
                </related>
        </func>
