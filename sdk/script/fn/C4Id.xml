<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<clonkDoc xmlns="https://clonkspot.org"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="https://clonkspot.org ../../../clonk.xsd" xml:lang="de">
	<func isAsync="false">
		<title>C4Id</title>
		<category>Script/Strings</category>
		<versions><version>4.6.5.0 CP</version></versions>
		<syntax>
			<rtype isReference="false">id</rtype>
			<params>
				<param isReference="false" isOptional="false"><type>string</type><name>szID</name><desc>Der String, der in eine id umgewandelt werden soll.</desc></param>
			</params>
		</syntax>
		<desc>Wandelt einen String in eine ID um.</desc>
		<examples>
			<example>
				<code>C4Id("ROCK")</code>
				<text>Gibt die ID 'ROCK' zurück.</text>
			</example>
		</examples>
	</func>
	<author>jwk</author><date>2002-04</date>
</clonkDoc>
